.mlddm {
	PADDING-RIGHT: 0px; PADDING-LEFT: 0px; PADDING-BOTTOM: 0px; MARGIN: 0px; PADDING-TOP: 0px; LIST-STYLE-TYPE: none; HEIGHT: 24px
}
.mlddm LI {
	BORDER-RIGHT: #fff 0px solid; PADDING-RIGHT: 0px; DISPLAY: inline; PADDING-LEFT: 0px; FLOAT: left; PADDING-BOTTOM: 0px; MARGIN: 0px; FONT: bold 11px arial; PADDING-TOP: 0px; LIST-STYLE-TYPE: none
}
.mlddm LI A {
	BORDER-RIGHT: 2px groove;PADDING-RIGHT: 10px; BORDER-TOP:0px groove; DISPLAY: block; PADDING-LEFT: 10px; PADDING-BOTTOM: 4px; MARGIN: 0px; BORDER-LEFT: #5970b2 0px solid; WIDTH: 60px; COLOR: #000000; PADDING-TOP: 4px; BORDER-BOTTOM: #5970b2 0px solid; TEXT-ALIGN: center; TEXT-DECORATION: none
}
.mlddm LI A#topnohover {
	 PADDING-RIGHT: 10px; BORDER-TOP: #5970b2 1px solid; DISPLAY: block; PADDING-LEFT: 10px; BACKGROUND: #5970b2; PADDING-BOTTOM: 4px; MARGIN: 0px; BORDER-LEFT: #5970b2 1px solid; WIDTH: 60px; COLOR: #fff; PADDING-TOP: 4px; BORDER-BOTTOM: #5970b2 1px solid; TEXT-ALIGN: center; TEXT-DECORATION: none
}
.mlddm LI A:hover {
	BACKGROUND: #FFA100;
}
.mlddm LI A#tophover {
	BACKGROUND: #abc9de
}
.mlddm UL {
	CLEAR: both; BORDER-RIGHT: #BCBCBD 1px solid; PADDING-RIGHT: 0px; BORDER-TOP: #BCBCBD 1px solid; PADDING-LEFT: 0px; Z-INDEX: 100; BACKGROUND: #7ba5c9; VISIBILITY: hidden; PADDING-BOTTOM: 0px; MARGIN: 8px 0px 0px; BORDER-LEFT: #BCBCBD 1px solid; PADDING-TOP: 0px; BORDER-BOTTOM: #BCBCBD 1px solid; POSITION: absolute
}
.mlddm UL LI {
	BORDER-TOP-WIDTH: 0px; PADDING-RIGHT: 0px; PADDING-LEFT: 0px; BORDER-LEFT-WIDTH: 0px; FLOAT: none; BORDER-BOTTOM-WIDTH: 0px; PADDING-BOTTOM: 0px; MARGIN: 0px; PADDING-TOP: 0px; BORDER-RIGHT-WIDTH: 0px
}
.mlddm UL LI A {
	BORDER-RIGHT: black 0px solid; PADDING-RIGHT: 13px; BORDER-TOP: black 0px solid; PADDING-LEFT: 13px; BACKGROUND-IMAGE:URL('../../images/bkg_sub_menu.gif'); PADDING-BOTTOM: 5px; MARGIN: 0px; FONT: 11px arial; BORDER-LEFT: black 0px solid; WIDTH: auto; COLOR: #000000; PADDING-TOP: 5px; BORDER-BOTTOM: black 0px solid; WHITE-SPACE: nowrap; TEXT-ALIGN: left; TEXT-DECORATION: none
}
.mlddm UL LI A:hover {
	COLOR: #fff
}
